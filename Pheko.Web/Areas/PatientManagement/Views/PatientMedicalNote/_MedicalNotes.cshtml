@using Pheko.WebPresentation.ViewModels;

@model PatientMedicalNoteViewModel

<div class="panel panel-primary">

    <div class="panel-body">
        <div class="formcontent container-fluid" ng-repeat="patientmedicalnote in patientmedicalnotes">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-sm-2 control-label">{{patientmedicalnote.MedicalNote_Name}}</label>
                    <div class="col-sm-10">
                        @Html.TextAreaFor(model => model.MedicalNoteValue, new { @class = "form-control", ng_model = "patientmedicalnote.MedicalNoteValue" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
