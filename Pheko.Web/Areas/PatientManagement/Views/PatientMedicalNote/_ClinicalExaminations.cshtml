@using Pheko.WebPresentation.ViewModels;

@model PatientClinicalExaminationViewModel

<div class="panel panel-primary">

    <div class="panel-body">
        <div class="formcontent container-fluid" ng-repeat="patientclinicalexamination in patientclinicalexaminations">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-sm-2 control-label">{{patientclinicalexamination.ClinicalExamination_Name}}</label>
                    <div class="col-sm-10">
                        @Html.TextAreaFor(model => model.ClinicalExaminationValue, new { @class = "form-control", ng_model = "patientclinicalexamination.ClinicalExaminationValue" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
